<!DOCTYPE html>
<html$if(lang)$ lang="$lang$"$endif$ class="no-js">
<head>
  <meta charset="utf-8">
  <meta name="generator" content="tao-doc">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  $for(author-meta)$
  <meta name="author" content="$author-meta$">
$endfor$
$if(date-meta)$
  <meta name="dcterms.date" content="$date-meta$">
$endif$
  <title>$if(title-prefix)$$title-prefix$ - $endif$$pagetitle$</title>
  <style type="text/css">code{white-space: pre;}</style>
  <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
  <script>var h = document.documentElement;
  h.className = h.className.replace('no-js', 'js')</script>
$if(quotes)$
  <style type="text/css">q { quotes: "“" "”" "‘" "’"; }</style>
$endif$
  <link href='http://fonts.googleapis.com/css?family=Roboto+Condensed:300|Roboto' rel='stylesheet' type='text/css'>
  <link rel="stylesheet" href="../assets/css/styles.css">
  $if(highlighting-css)$
  <style type="text/css">
$highlighting-css$
  </style>
$endif$
$for(css)$
  <link rel="stylesheet" href="$css$">
$endfor$
$if(math)$
  $math$
$endif$
$for(header-includes)$
  $header-includes$
$endfor$
</head>
<body>
<div class="wrapper content-wrap">

  <header class="flex-area">
    <a href="/" class="block sidebar">
      <h1>
        <span class="viewport-hidden">Open Assessment Technologies - TAO User Guide</span>
        <img src="../assets/img/header-logo.png" alt="Open Assessment Technologies - TAO User Guide"/>
        <span class="user-guide">User Guide</span>

      </h1>
    </a>

    <div class="search-area">
      <form class="rgt" action="../search/">
        <input type="search" name="q" placeholder="Search the User Guide" />
        <button type="submit"><span class="icon-search"></span>Search</button>
      </form>
    </div>

      <script type="text/javascript">

          var _gaq = _gaq || [];
          _gaq.push(['_setAccount', 'UA-28412224-1']);
          _gaq.push(['_setDomainName', '.taotesting.com']);
          _gaq.push(['_gat._forceSSL']);

          _gaq.push(['_trackPageview']);

          (function () {
              var ga = document.createElement('script');
              ga.type = 'text/javascript';
              ga.async = true;
              ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
              var s = document.getElementsByTagName('script')[0];
              s.parentNode.insertBefore(ga, s);
          })();

      </script>

  </header>



  <div class="clearfix flex-area">
    $for(include-before)$
    $include-before$
    $endfor$
    <main>

<div class="guide-content">
  $body$
</div>
$for(include-after)$
$include-after$
$endfor$
    </main>
  </div>

</div>
<footer class="wrapper">

    <div class="flt-col-6">
        <a href="http://taotesting.com" target="_blank">OAT Website</a>
        <a href="http://www.taotesting.com/get-tao/official-tao-packages/" target="_blank">Get TAO</a>
    </div>

    <div class="flt-col-6"><span class="copyrights">© 2015 Open Assessment Technologies S.A. All rights reserved.</span></div>
    <script src="../assets/js/vendor/jquery.1.10.2.min.js"></script>
  <script src="../assets/js/main.js"></script>

</footer>

</body>
</html>